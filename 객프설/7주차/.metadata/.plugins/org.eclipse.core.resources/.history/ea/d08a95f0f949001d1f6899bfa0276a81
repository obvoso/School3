package Test;

class Contacts {
	
	String name;
	String tel;
	String email;
	static int count = 0;
	
	Contacts(){
		name = null;
		tel = null;
		email = null;
		++count;
	}
	
	Contacts(String n, String t, String e) {
		super();
		this.name = n;
		this.tel = t;
		this.email = e;
		++count;
	}
}

public class Ex06 {
	public static void main(String[] args) {
		
	}
}
