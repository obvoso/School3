package Test;

class Circle {
	
	int radius;
	
	Circle(){
		radius = (int)Math.random() * 100;
	}

	@Override
	public String toString() {
		return "Circle [radius=" + radius + "]";
	}
	
}

public class Ex05 {
	
	public static void main(String[] args) {
		
		Circle[] cArr = new Circle[3];
		
		for (int i = 0; i < cArr.length; i++) {
			System.out.println(cArr[i]);
		}
	}
}
