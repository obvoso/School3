package Ex;

class Vehicle {
	
	String color;
	int speed;
	
	public Vehicle(String c, int s) {
		color = c;
		speed = s;
	}
	
	void show() {
		
	}
}

class Car extends Vehicle {
	int displacement;
	int gears;
	
	public Car(String c, int s, int d, int g) {
		super(c, s);
		displacement = d;
		gears = g;
	}
}

