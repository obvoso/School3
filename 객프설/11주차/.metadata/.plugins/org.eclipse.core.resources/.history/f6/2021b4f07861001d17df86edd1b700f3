package Ex;

interface Human {
	void eat();
	void print();
}

class Worker implements Human {

	@Override
	public void eat() {
		System.out.println("빵을 먹습니다.");
	}
	@Override
	public void print() {
		System.out.println("인간입니다.");
	}
	void echo() {
		System.out.println("야호!!!");
	}
}

class Student1 implements Human {

	@Override
	public void eat() {
		System.out.println("빵을 먹습니다.");
	}
	@Override
	public void print() {
		System.out.println("도시락을 먹습니다.");
	}
	void echo() {
		System.out.println("야호!!!");
	}
}

public class Ex04 {
	public static void main(String[] args) {
		
	}
}
