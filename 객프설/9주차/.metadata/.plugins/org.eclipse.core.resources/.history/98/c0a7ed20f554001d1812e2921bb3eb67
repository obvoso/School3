package Succession;

class Person2 {
	String name;

	public Person2() {
		
	}
	
	public Person2(String name) {
		super();
		this.name = name;
	}
}

class Employee extends Person2{
	String id;
	public Employee() {
		super();
	}

	public Employee(String id) {
		super();
		this.id = id;
	}
	public Employee(String name, String id) {
		super();
		this.id = id;
	}
}

public class EmployeeTest {
	public static void main(String[] args) {
		
	}

}
